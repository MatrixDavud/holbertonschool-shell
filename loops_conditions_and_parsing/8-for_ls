#!/usr/bin/env bash
# This script will displayt the content of the current directory, in a list format, where only the part of the name after the first dash is displayed
for file in [0-9]*-*; do
    # Skip if no files match (avoid literal "[0-9]*-*" being processed)
    [[ -e "$file" ]] || continue

    # Extract the part after digits and dash
    filtered_name=$(echo "$file" | sed -E 's/^[a-zA-Z0-9]+-//')

    # Print in list format
    echo "$filtered_name"
done 
